{
    "name": "lisys-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/compatible": "1.0.0",
        "@apollo/react-hooks": "3.1.3",
        "@sentry/browser": "5.9.1",
        "antd": "4.0.0",
        "apollo": "2.21.0",
        "apollo-cache-inmemory": "1.6.3",
        "apollo-client": "2.6.4",
        "apollo-link-context": "1.0.19",
        "apollo-link-error": "1.1.12",
        "apollo-link-http": "1.5.16",
        "enquire-js": "0.2.1",
        "graphql-tag": "2.10.1",
        "lodash": "4.17.15",
        "moment": "2.26.0",
        "nprogress": "0.2.0",
        "rc-drawer": "3.0.2",
        "react": "16.12.0",
        "react-custom-scrollbars": "4.2.1",
        "react-dom": "16.12.0",
        "react-loadable": "5.5.0",
        "react-router-dom": "5.1.2",
        "rxjs": "6.5.3"
    },
    "devDependencies": {
        "@apollo/react-testing": "3.1.4",
        "@testing-library/react": "10.0.4",
        "@testing-library/react-hooks": "3.2.1",
        "@types/enzyme": "3.10.5",
        "@types/enzyme-adapter-react-16": "1.0.6",
        "@types/jest": "24.0.23",
        "@types/lodash": "4.14.149",
        "@types/node": "12.12.12",
        "@types/nprogress": "0.2.0",
        "@types/react": "16.9.12",
        "@types/react-custom-scrollbars": "4.0.6",
        "@types/react-dom": "16.9.4",
        "@types/react-loadable": "5.5.2",
        "@types/react-router-dom": "5.1.2",
        "babel-plugin-import": "1.12.2",
        "customize-cra": "0.9.1",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.2",
        "eslint-plugin-prettier": "3.1.1",
        "jest-cli": "25.4.0",
        "less": "3.10.3",
        "less-loader": "5.0.0",
        "mock-apollo-client": "0.3.0",
        "node-sass": "4.14.1",
        "prettier": "1.19.1",
        "react-app-rewired": "2.1.5",
        "react-scripts": "3.4.0",
        "sass-loader": "8.0.0",
        "typescript": "3.7.2"
    },
    "scripts": {
        "start": "react-app-rewired start",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test",
        "test-coverage": "react-app-rewired test --coverage --watchAll=false --coverageDirectory=output/coverage/jest",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "coverageReporters": [
            "text",
            "lcov"
        ],
        "collectCoverageFrom": [
            "!**/*.ts",
            "!src/index.tsx",
            "!src/serviceWorker.ts",
            "!src/app/App.tsx",
            "!src/app/Routes.tsx",
            "!src/app/shell/components/Header/Header.tsx",
            "!src/core/**",
            "!src/shared/components/Upload/**",
            "!src/shared/graphql/**",
            "!**/node_modules/**",
            "!**/vendor/**"
        ],
        "coveragePathIgnorePatterns": [
            "Page.tsx"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 50,
                "functions": 20,
                "lines": 50,
                "statements": 50
            }
        }
    }
}
